# Environment Variables Template
# Copy this file to .env and fill in your actual API keys

# Anthropic API Configuration - SET IN PRODUCTION ONLY
# ANTHROPIC_API_KEY=disabled_for_local_development

# Google Cloud API (if needed for additional OCR)
GOOGLE_CLOUD_API_KEY=your_google_api_key_here

# Application Configuration
NODE_ENV=development
REACT_APP_VERSION=1.0.0

# Optional: Rate limiting and caching
RATE_LIMIT_WINDOW=3600000
RATE_LIMIT_MAX=100
